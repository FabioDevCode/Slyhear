        <li data-tracks-id="{{this.id}}">
            {{#if this.image}}
                <img src="data:image/jpeg;base64,{{this.image}}" alt="{{this.title}}">
            {{else}}
                <img src="/img/_default.jpg" alt="Jaquette par dÃ©faut Slyhear">
            {{/if}}
            <h2>
                {{this.title}}
            </h2>
        </li>
        {{/each}}
    </ul>

</div>